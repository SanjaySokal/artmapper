{
  "name": "springboot-ts",
  "version": "1.0.0",
  "description": "Spring Boot clone for Node.js with TypeScript, JPA, Lombok, and MySQL support",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/example/main.ts",
    "start": "node dist/example/main.js",
    "example": "ts-node-dev --respawn --transpile-only src/example/main.ts",
    "test": "jest"
  },
  "keywords": [
    "springboot",
    "typescript",
    "orm",
    "jpa",
    "lombok",
    "mysql",
    "decorators",
    "dependency-injection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.1.14",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "express": "^4.18.2"
  }
}

